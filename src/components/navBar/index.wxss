.nav {
  background: #fff;
  position: fixed;
  top: 0;
  width: 100%;
  z-index: 9999999;
}
.nav::after {
  content: '';
  overflow: hidden;
  width: 100%;
  height: 0;
}

.navbar {
  display: flex;
  position: relative;
}
.navbar image {
  height: 16px;
  width: 16px;
}

.left-container {
  /*background: rgba(255,255,255,0.60);*/
  width: 87px;
  height: 32px;
  position: absolute;
  top: 6px;
  left: 10px;
  display: flex;
  z-index: 10;
}
.left-container .left-border {
  position: absolute;
  top: 0;
  left: 0;
  height: 64px;
  width: 174px;
  border: 1px solid rgba(0,0,0,0.08);
  border-radius: 32px;
  transform-origin: 0 0;
  transform: scale(0.5);
  z-index: -1;
}
.left-container .back-icon, .left-container .home-icon{
  flex: 1;
  position: relative;
  display: flex;
  justify-content: center;
  align-items: center;
}
.left-container .left-divide {
  width: 1px;
  /*background: rgba(0,0,0,0.2);*/
  height: 20px;
  margin-top: 6px;
}

.nav-title {
  display: flex;
  align-items: center;
  justify-content: center;
  position: relative;
  bottom: 0;
  height: 100%;
  width: 100%;
  box-sizing: border-box;
}
.nav-title view {
  font-family: PingFangSC-Medium;
  font-size: 37rpx;
  color: #000;
  letter-spacing: 0;
  text-overflow: ellipsis;
  white-space: nowrap;
  overflow: hidden;
  text-align: center;
  line-height: 44px;
}

.no-border {
  height: 32px;
  position: absolute;
  top: 6px;
  left: 10px;
  display: flex;
  align-items: center;
  justify-content: center;
  z-index: 10;
  width: 43px;
}

.home-border {
  position: absolute;
  top: 0;
  left: 0;
  height: 64px;
  width: 88px;
  border: 1px solid rgba(0,0,0,0.08);
  border-radius: 32px;
  transform-origin: 0 0;
  transform: scale(0.5);
  z-index: -1;
}

.no-border .back-icon, .no-border .home-icon {
  flex: 1;
}
.no-border .home-icon {
  padding-left: 14px;
}
